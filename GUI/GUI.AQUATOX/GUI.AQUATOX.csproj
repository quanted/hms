<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows8.0</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <StartupObject>GUI.AQUATOX.Program</StartupObject>
    <ApplicationIcon>aquatox_Icon.ico</ApplicationIcon>
	<AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
	<AppendRuntimeIdentifierToOutputPath>false</AppendRuntimeIdentifierToOutputPath>
  
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <Optimize>false</Optimize>
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <CheckForOverflowUnderflow>True</CheckForOverflowUnderflow>
  </PropertyGroup>
	<PropertyGroup>
		<ErrorOnDuplicatePublishOutputFiles>false</ErrorOnDuplicatePublishOutputFiles>
		<PlatformTarget>AnyCPU</PlatformTarget>
		<SupportedOSPlatformVersion>8.0</SupportedOSPlatformVersion>
		<ProduceReferenceAssembly>False</ProduceReferenceAssembly>
		<BaseOutputPath>bin\</BaseOutputPath>
		<OutputPath>bin\Program\</OutputPath>

	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
	  <CheckForOverflowUnderflow>True</CheckForOverflowUnderflow>
	</PropertyGroup>
	
	<PropertyGroup>
	<ValidateExecutableReferencesMatchSelfContained>false</ValidateExecutableReferencesMatchSelfContained>
	<NoWin32Manifest>true</NoWin32Manifest>  
	</PropertyGroup>
	
  <ItemGroup>
    <!-- Include 2D_Inputs files in the project and set their output directory -->
    <Content Include="2D_Inputs\**">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Link>..\2D_Inputs\%(RecursiveDir)%(Filename)%(Extension)</Link>
    </Content>	
  </ItemGroup>
	
	<ItemGroup>
    <!-- Include Docs files in the project and set their output directory -->
    <Content Include="Docs\**">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Link>..\Docs\%(RecursiveDir)%(Filename)%(Extension)</Link>
    </Content>	
  </ItemGroup>
	
	<ItemGroup>
    <!-- Include Studies files in the project and set their output directory -->
    <Content Include="Studies\**">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Link>..\Studies\%(RecursiveDir)%(Filename)%(Extension)</Link>
    </Content>	
  </ItemGroup>
		
    <ItemGroup>
    <!-- Include DB files in the project and set their output directory -->
    <Content Include="DB\**">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Link>..\DB\%(RecursiveDir)%(Filename)%(Extension)</Link>
    </Content>	
  </ItemGroup>
	
	<ItemGroup>
		<!-- Include html files in the project and set their output directory -->
		<Content Include="html\**">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
			<Link>html\%(RecursiveDir)%(Filename)%(Extension)</Link>
		</Content>
	</ItemGroup>


	<ItemGroup>
		<!-- Include x86 7zip files in the project and set their output directory -->
		<Content Include="x86\**">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
			<Link>x86\%(RecursiveDir)%(Filename)%(Extension)</Link>
		</Content>
	</ItemGroup>

	<ItemGroup>
		<!-- Include x64 7zip files in the project and set their output directory -->
		<Content Include="x64\**">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
			<Link>x64\%(RecursiveDir)%(Filename)%(Extension)</Link>
		</Content>
	</ItemGroup>

	<ItemGroup>
    <PackageReference Include="7z.Libs" Version="23.1.0" />
    <PackageReference Include="Microsoft.Web.WebView2" Version="1.0.2277.86" />
    <PackageReference Include="SDSLite" Version="2.0.3" />
    <PackageReference Include="Microsoft.DotNet.UpgradeAssistant.Extensions.Default.Analyzers" Version="0.4.355802">
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.Windows.Compatibility" Version="8.0.1" />
    <PackageReference Include="SevenZipSharp.Interop" Version="19.1.0" />
	  <PackageReference Include="System.Windows.Forms.DataVisualization" Version="1.0.0-prerelease.20110.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Data.Simulate.AQUATOX\Data.Simulate.AQUATOX.csproj" />
    <ProjectReference Include="..\..\Data\Data.csproj" />
    <ProjectReference Include="..\..\Hawqs\Hawqs.csproj" />
    <ProjectReference Include="..\..\Web.Services\Web.Services.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="RadioButtonForm.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="GraphSetup.cs" />

    <Compile Update="ListForm.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="MultiSegForm.cs" />
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
    <Compile Update="TrophMatrix.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="LoadingsForm.cs">
      <SubType>Form</SubType>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

	<ItemGroup>
		<None Update="2D_Inputs\HAWQS_data\COMID_HUC14_Unique.sqlite">
		  <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
		<None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>


</Project>
